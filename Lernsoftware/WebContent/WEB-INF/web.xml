<web-app xmlns="http://java.sun.com/xml/ns/javaee"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
      version="3.0"> 
	<display-name>Lernsoftware</display-name>
	<servlet>
		<servlet-name>index</servlet-name>
		<servlet-class>de.adv_boeblingen.seeegerj.amed.lernoftware.view.RootRedirect</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index_ignored</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index_ignored</welcome-file>
	</welcome-file-list>
</web-app>
	